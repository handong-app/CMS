meta {
  name: courseUpdate
  type: http
  seq: 4
}

patch {
  url: http://{{host}}/api/v1/clubs/{{clubSlug}}/courses/{{courseSlug}}
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "title": "수정된 코스 제목",
    "description": "수정된 코스 설명입니다.",
    "pictureUrl": "https://example.com/thumbnails/updated-course.jpg",
    "isVisible": false
  }
}

vars:pre-request {
  courseSlug: test-course-aaa
  clubSlug: example-club-slug
  host: localhost:8080
}
